  OPUS
  PCM
  WAV
}

enum OpenAITTSModel {
  GPT_4O_MINI_TTS
  TTS_1
  TTS_1_HD
}

enum OpenAITTSVoice {
  ALLOY
  ASH
  BALLAD
  CORAL
  ECHO
  FABLE
  NOVA
  ONYX
  SAGE
  SHIMMER
}

