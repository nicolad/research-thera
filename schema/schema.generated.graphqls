  speed: Float
  text: String!
  uploadToCloud: Boolean
  voice: OpenAITTSVoice
}

type GenerateOpenAIAudioResult {
  audioBuffer: String
  audioUrl: String
  duration: Float
  key: String
  message: String
  sizeBytes: Int
