}

type GenerateQuestionsResult {
  jobId: String
  message: String
  questions: [TherapeuticQuestion!]!
  success: Boolean!
}

type GenerateResearchResult {
  count: Int
  jobId: String
  message: String
  success: Boolean!
}

type GenerationJob {
  createdAt: String!
  error: JobError
  goalId: Int!
  id: String!
  progress: Float!
  result: JobResult
  status: JobStatus!
  storyId: Int
  type: JobType!
  updatedAt: String!
  userId: String!
}

type Goal {
  createdAt: String!
  createdBy: String!
  description: String
  familyMemberId: Int!
  id: Int!
  notes: [Note!]!
  questions: [TherapeuticQuestion!]!
  research: [Research!]!
  slug: String
  status: String!
  stories: [GoalStory!]!
  therapeuticText: String
  therapeuticTextGeneratedAt: String
  therapeuticTextLanguage: String
  title: String!
  updatedAt: String!
  userStories: [Story!]!
}

type GoalStory {
  audioAssets: [AudioAsset!]!
  createdAt: String!
  goalId: Int!
  id: Int!
  language: String!
  minutes: Int!
  segments: [TextSegment!]!
  text: String!
  updatedAt: String!
}

type JobError {
  code: String
  details: String
  message: String!
}

type JobResult {
  assetId: String
  audioUrl: String
  count: Int
  manifestUrl: String
  questions: [TherapeuticQuestion!]
  segmentUrls: [String!]
  text: String
}

enum JobStatus {
  FAILED
  RUNNING
  SUCCEEDED
}

enum JobType {
  AUDIO
  LONGFORM
  QUESTIONS
  RESEARCH
}

type Mutation {
  buildClaimCards(input: BuildClaimCardsInput!): BuildClaimCardsResult!
  checkNoteClaims(input: CheckNoteClaimsInput!): CheckNoteClaimsResult!
  createGoal(input: CreateGoalInput!): Goal!
  createNote(input: CreateNoteInput!): Note!
  createStory(input: CreateStoryInput!): Story!
  deleteClaimCard(id: ID!): Boolean!
  deleteGoal(id: Int!): DeleteGoalResult!
  deleteNote(id: Int!): DeleteNoteResult!
  deleteResearch(goalId: Int!): DeleteResearchResult!
  deleteStory(id: Int!): DeleteStoryResult!
  deleteTherapeuticQuestions(goalId: Int!): DeleteQuestionsResult!
  generateAudio(goalId: Int!, language: String, storyId: Int, text: String, voice: String): GenerateAudioResult!
  generateLongFormText(goalId: Int!, language: String, minutes: Int): GenerateLongFormTextResult!
  generateOpenAIAudio(input: GenerateOpenAIAudioInput!): GenerateOpenAIAudioResult!
  generateResearch(goalId: Int!): GenerateResearchResult!
  generateTherapeuticQuestions(goalId: Int!): GenerateQuestionsResult!
  refreshClaimCard(id: ID!): ClaimCard!
  updateGoal(id: Int!, input: UpdateGoalInput!): Goal!
  updateNote(id: Int!, input: UpdateNoteInput!): Note!
  updateStory(id: Int!, input: UpdateStoryInput!): Story!
}

type Note {
  claimCards: [ClaimCard!]
